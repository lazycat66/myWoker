<%- include header %>
<p>
    <%= content %>
</p>
<% if(list.length > 0){ %>
    <ul>
        <% for(var i=0; i < list.length; i++){ %>
            <li>
                <%= list[i] %>
            </li>
        <% } %>
    </ul>
<% } else { %>
    <p>
        small list
    </p>
<% } %>
<%- include footer %>
